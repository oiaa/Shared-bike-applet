/* projects/scanCode/scanCode.wxss */
.container{
  width: 100%;
  height: 100vh;
  display: flex;
  align-items: center;
  flex-direction: column;
  justify-content: space-between;
}
.pass-title,.tips{
  flex: 1;
  width: 100%;
  text-align: center;
  padding: 40rpx; 
  font-size: 32rpx;
}
.pass-content{
  flex: 8;
  width: 100%;
  text-align: center;
  font-size: 200rpx;
  line-height: 200rpx;
  padding-top: 200rpx;
  font-weight: 600;
}
.tips-href{
  color: #b9dd08;
}